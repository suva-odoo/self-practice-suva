<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
<record id="travel_place_types_action" model="ir.actions.act_window">
    <field name="name">Place Types</field>
    <field name="res_model">travel.place.types</field>
    <field name="view_mode">tree,form</field>
</record>


<record id="travel_place_types_view_tree" model="ir.ui.view">
    <field name="name">travel.place.types.tree</field>
    <field name="model">travel.place.types</field>
    <field name="arch" type="xml">
       <tree>
          <field name="name"/>
       </tree>
    </field>
</record>

<record id="travel_place_types_view_form" model="ir.ui.view">
    <field name="name">travel.place.types.form</field>
    <field name="model">travel.place.types</field>
    <field name="arch" type="xml">
    <form>
      <sheet>
         <field name="name"/>
      </sheet>
    </form>
    </field>
</record>
</odoo>